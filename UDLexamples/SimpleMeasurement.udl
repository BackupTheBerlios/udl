
#Config the measurement
[Measurement]
#Sample Time in ms
SampleTime = 1000
#MeasMode 'Continious' -> Log all MeasDev's 
Mode = Continious
#Count of measurements to take. (-1 never stop measuring)
SampleCount = 1000


#Config the output chanel
[Out]
OutDataBase = FileCsv
OutFileName = C:\test_01.csv


#Config the devices
[Devices]
MeasDev_1 = Test.dll
MeasDev_2 = Test.dll

[MeasDev_1]
NiceName = TestMultimeter01 
Args = TestTest

[MeasDev_2]
NiceName = A Second Test Device
Args = TestTest
